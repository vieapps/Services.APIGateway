<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="net.vieapps.repositories" type="net.vieapps.Components.Utility.AppConfigurationSectionHandler, VIEApps.Components.Utility" />
		<section name="dbProviderFactories" type="net.vieapps.Components.Utility.AppConfigurationSectionHandler, VIEApps.Components.Utility" />
		<section name="cache" type="net.vieapps.Components.Caching.CacheConfigurationSectionHandler, VIEApps.Components.Caching" />
	</configSections>
	<appSettings>
		<add key="vieapps:RouterAddress" value="ws://24.4.77.3:16429/" />
		<add key="vieapps:CacheExpirationTime" value="45" />
		<add key="vieapps:CacheProvider" value="Redis" />
		<add key="vieapps:SystemAdministrators" value="bf2d8350b8a34345877cc7e35148d47c" />
		<add key="vieapps:HttpFilesUri" value="http://local-afs.vieapps.net" />
		<add key="vieapps:HttpUsersUri" value="http://local-aid.vieapps.net" />
		<add key="vieapps:LogsPath" value="\\Mac\Home\Documents\VIE Apps\log-files" />
		<add key="vieapps:BookFilesPath" value="\\Mac\Home\Documents\VIE Apps\data-files\books" />
		<add key="vieapps:LibraryFilesPath" value="\\Mac\Home\Documents\VIE Apps\data-files\libraries" />
	</appSettings>
	<connectionStrings>
		<add name="MongoDB" providerName="MongoDB" connectionString="mongodb://sa:sa%4012345@24.4.77.2/admin" />
		<add name="MySQL" providerName="MySql.Data.MySqlClient" connectionString="server=24.4.77.2;database={database};uid=root;pwd=sa@12345;max pool size=200;min pool size=1;" />
		<add name="PostgreSQL" providerName="Npgsql" connectionString="server=24.4.77.2;database={database};uid=root;pwd=sa@12345;max pool size=200;min pool size=1;" />
		<add name="SQLServer" providerName="System.Data.SqlClient" connectionString="server=24.4.77.4;database={database};uid=sa;pwd=sa@12345;max pool size=200;min pool size=1;" />
	</connectionStrings>
	<net.vieapps.repositories ensureSchemas="true" ensureIndexes="true">
		<dataSources>
			<dataSource name="VIEApps-Users-MongoDB" databaseName="VIEApps-Users" connectionStringName="MongoDB" mode="NoSQL" />
			<dataSource name="VIEApps-Users-MySQL" databaseName="VIEApps-Users" connectionStringName="MySQL" mode="SQL" />
			<dataSource name="VIEApps-Users-SQLServer" databaseName="VIEApps-Users" connectionStringName="SQLServer" mode="SQL" />
			<dataSource name="VIEApps-Files-MongoDB" databaseName="VIEApps-Files" connectionStringName="MongoDB" mode="NoSQL" />
			<dataSource name="VIEApps-Files-MySQL" databaseName="VIEApps-Files" connectionStringName="MySQL" mode="SQL" />
			<dataSource name="VIEApps-Files-SQLServer" databaseName="VIEApps-Files" connectionStringName="SQLServer" mode="SQL" />
			<dataSource name="VIEApps-Core-MongoDB" databaseName="VIEApps-Core" connectionStringName="MongoDB" mode="NoSQL" />
			<dataSource name="VIEApps-Core-MySQL" databaseName="VIEApps-Core" connectionStringName="MySQL" mode="SQL" />
			<dataSource name="VIEApps-Core-SQLServer" databaseName="VIEApps-Core" connectionStringName="SQLServer" mode="SQL" />
			<dataSource name="VIEApps-Marketplaces-MongoDB" databaseName="VIEApps-Marketplaces" connectionStringName="MongoDB" mode="NoSQL" />
			<dataSource name="VIEApps-Marketplaces-MySQL" databaseName="VIEApps-Marketplaces" connectionStringName="MySQL" mode="SQL" />
			<dataSource name="VIEApps-Marketplaces-SQLServer" databaseName="VIEApps-Marketplaces" connectionStringName="SQLServer" mode="SQL" />
			<dataSource name="VIEApps-Libraries-MongoDB" databaseName="VIEApps-Libraries" connectionStringName="MongoDB" mode="NoSQL" />
			<dataSource name="VIEApps-Libraries-MySQL" databaseName="VIEApps-Libraries" connectionStringName="MySQL" mode="SQL" />
			<dataSource name="VIEApps-Libraries-SQLServer" databaseName="VIEApps-Libraries" connectionStringName="SQLServer" mode="SQL" />
			<dataSource name="VIEApps-Books-MongoDB" databaseName="VIEApps-Books" connectionStringName="MongoDB" mode="NoSQL" />
			<dataSource name="VIEApps-Books-MySQL" databaseName="VIEApps-Books" connectionStringName="MySQL" mode="SQL" />
			<dataSource name="VIEApps-Books-SQLServer" databaseName="VIEApps-Books" connectionStringName="SQLServer" mode="SQL" />
		</dataSources>
		<repository type="net.vieapps.Services.Users.Repository`1, VIEApps.Services.Users" primaryDataSource="VIEApps-Users-MongoDB" secondaryDataSource="VIEApps-Users-MySQL" syncDataSources="" />
		<repository type="net.vieapps.Services.Files.Repository`1, VIEApps.Services.Files" primaryDataSource="VIEApps-Files-MongoDB" secondaryDataSource="VIEApps-Files-MySQL" syncDataSources="" />
		<repository type="net.vieapps.Services.Systems.Repository`1, VIEApps.Services.Systems" primaryDataSource="VIEApps-Core-MongoDB" secondaryDataSource="VIEApps-Core-MySQL" syncDataSources="" />
		<repository type="net.vieapps.Services.Marketplaces.Repository`1, VIEApps.Services.Marketplaces" primaryDataSource="VIEApps-Marketplaces-MongoDB" secondaryDataSource="VIEApps-Marketplaces-MySQL" syncDataSources="" />
		<repository type="net.vieapps.Services.Libraries.Repository`1, VIEApps.Services.Libraries" primaryDataSource="VIEApps-Libraries-MongoDB" secondaryDataSource="VIEApps-Libraries-MySQL" syncDataSources="" />
		<repository type="net.vieapps.Services.Books.Repository`1, VIEApps.Services.Books" primaryDataSource="VIEApps-Books-MongoDB" secondaryDataSource="VIEApps-Books-MySQL" syncDataSources="" />
	</net.vieapps.repositories>
	<dbProviderFactories>
		<add invariant="System.Data.SqlClient" type="System.Data.SqlClient.SqlClientFactory, System.Data.SqlClient" name="Microsoft SQL Server" description="Data Provider for Microsoft SQL Server" />
		<add invariant="MySql.Data.MySqlClient" type="MySql.Data.MySqlClient.MySqlClientFactory, MySqlConnector" name="MySQL" description="Data Provider for MySQL" />
		<add invariant="Npgsql" type="Npgsql.NpgsqlFactory, Npgsql" name="PostgreSQL" description="Data Provider for PostgreSQL" />
		<add invariant="Oracle.ManagedDataAccess.Client" type="Oracle.ManagedDataAccess.Client.OracleClientFactory, Oracle.ManagedDataAccess" name="Oracle" description="Data Provider for Oracle RDBMS" />
	</dbProviderFactories>
	<cache>
		<servers>
			<add address="24.4.77.2" port="26429" type="Redis" />
			<add address="24.4.77.2" port="36429" type="Memcached" />
			<!--
			<add address="24.4.77.3" port="26429" type="Redis" />
			<add address="24.4.77.3" port="36429" type="Memcached" />
			<add address="24.4.77.4" port="26429" type="Redis" />
			<add address="24.4.77.4" port="36429" type="Memcached" />
			-->
		</servers>
		<socketPool minPoolSize="10" maxPoolSize="100" connectionTimeout="00:00:05" deadTimeout="00:05:00" />
		<options name="VIEApps NGX" allowAdmin="false" version="3.0" connectTimeout="4000" syncTimeout="2000" />
	</cache>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.1" />
	</startup>
