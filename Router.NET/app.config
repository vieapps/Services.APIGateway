<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<appSettings>
		<add key="Address" value="ws://0.0.0.0:16429/"/>
		<add key="Realm" value="VIEAppsRealm"/>
		<add key="SslCertificate:FilePath" value="/path-to/pfx-file"/>
		<add key="SslCertificate:Password" value="pfx-password"/>
		<add key="SslProtocol" value="Tls12"/>
		<add key="StatisticsWebSocketServer:Enable" value="true"/>
		<add key="StatisticsWebSocketServer:Port" value="56429"/>
	</appSettings>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
	</startup>
</configuration>
