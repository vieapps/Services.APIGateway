{
	"Logging": {
		"IncludeScopes": false,
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "None"
		}
	},
	"Cache": {
		"Provider": "Redis",
		"RegionName": "VIEApps-Services-APIGateway-Http",
		"ExpirationTime": 25,
		"Servers": [
			{
				"Address": "cache.vieapps.com",
				"Port": 26429,
				"Type": "Redis"
			},
			{
				"Address": "cache.vieapps.com",
				"Port": 36429,
				"Type": "Memcached"
			}
		],
		"SocketPool": {
			"MinPoolSize": 10,
			"MaxPoolSize": 512,
			"DeadTimeout": "00:01:00",
			"ConnectionTimeout": "00:00:05",
			"ReceiveTimeout": "00:00:01"
		},
		"Options":  "name=VIEApps NGX,abortConnect=false,allowAdmin=false,connectTimeout=5000,syncTimeout=2000"
	}
}