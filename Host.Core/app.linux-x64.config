<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="net.vieapps.cache" type="net.vieapps.Components.Caching.CacheConfigurationSectionHandler,VIEApps.Components.Caching" />
		<section name="net.vieapps.dbproviders" type="net.vieapps.Components.Utility.AppConfigurationSectionHandler,VIEApps.Components.Utility" />
		<section name="net.vieapps.repositories" type="net.vieapps.Components.Utility.AppConfigurationSectionHandler,VIEApps.Components.Utility" />
		<section name="net.vieapps.services.iplocations.providers" type="net.vieapps.Components.Utility.AppConfigurationSectionHandler,VIEApps.Components.Utility" />
	</configSections>
	<appSettings>
		<add key="vieapps:Router:Uri" value="ws://router.vieapps.com:16429" />
		<add key="vieapps:HttpUri:APIs" value="https://apis.vieapps.com" />
		<add key="vieapps:HttpUri:Files" value="https://fs.vieapps.com" />
		<add key="vieapps:HttpUri:Portals" value="https://portals.vieapps.com" />
		<add key="vieapps:HttpUri:Passports" value="https://id.vieapps.com" />
		<add key="vieapps:Path:APIGateway:Controller" value="/media/psf/Home/Documents/VIEApps.NGX/Builds/linux-x64/APIGateway/Controller" />
		<add key="vieapps:Path:Logs" value="/media/psf/Home/Documents/VIEApps.NGX/Builds/linux-x64/@Logs" />
		<add key="vieapps:Path:Temp" value="/media/psf/Home/Documents/VIEApps.NGX/Data/temp" />
		<add key="vieapps:Path:StaticFiles" value="/media/psf/Home/Documents/VIEApps.NGX/Data/statics" />
		<add key="vieapps:Path:Books" value="/media/psf/Home/Documents/VIEApps.NGX/Data/books" />
		<add key="vieapps:Keys:Encryption" value="VIEApps-15D17AFB-NGX-A2A2-Services-43FC-Encryption-A482-Key-6B240D2CD323" />
		<add key="vieapps:Keys:Validation" value="VIEApps-3068B9D6-NGX-FEFF-Services-4176-Validation-8736-Key-D046804266D3" />
		<add key="vieapps:Keys:Activation" value="VIEApps-F537DBC3-NGX-5189-Services-4CAA-Activation-86B3-Key-DCF1E15283A9" />
		<add key="vieapps:Keys:Authentication" value="VIEApps-500E47DD-NGX-177B-Services-43E7-Authentication-B4A8-Key-4533E739AF39" />
		<add key="vieapps:Keys:ECC" value="" />
		<add key="vieapps:Keys:RSA" value="" />
		<add key="vieapps:Logs:Level" value="Debug" />
		<add key="vieapps:Logs:ShowResults" value="true" />
		<add key="vieapps:Logs:ShowStacks" value="true" />
		<add key="vieapps:Users:SystemAdministrators" value="bf2d8350b8a34345877cc7e35148d47c" />
		<add key="vieapps:OTPs:Issuer" value="VIEApps NGX" />
		<add key="vieapps:Books:Crawler-MaxPages" value="1" />
		<add key="vieapps:Books:Crawler-VnThuQuan" value="false" />
		<add key="vieapps:Books:Crawler-ISach" value="false" />
		<add key="vieapps:Books:Crawler-RunAtStartup" value="false" />
	</appSettings>
	<connectionStrings>
		<add name="MongoDB" providerName="MongoDB" connectionString="mongodb://sa:sa%4012345@db.vieapps.com/admin" />
		<add name="MySQL" providerName="MySQL" connectionString="server=db.vieapps.com;database={database};uid=root;pwd=sa@12345;max pool size=200;min pool size=1;" />
		<add name="PostgreSQL" providerName="PostgreSQL" connectionString="server=db.vieapps.com;database={database};uid=root;pwd=sa@12345;maximum pool size=200;minimum pool size=1;" />
		<add name="SQLServer" providerName="SQLServer" connectionString="server=db.vieapps.com;database={database};uid=sa;pwd=sa@12345;max pool size=200;min pool size=1;" />
	</connectionStrings>
	<net.vieapps.dbproviders>
		<add name="MySQL" type="MySql.Data.MySqlClient.MySqlClientFactory,MySqlConnector" description="Data Provider for MySQL" />
		<add name="PostgreSQL" type="Npgsql.NpgsqlFactory,Npgsql" description="Data Provider for PostgreSQL" />
		<add name="SQLServer" type="System.Data.SqlClient.SqlClientFactory,System.Data.SqlClient" description="Data Provider for SQL Server" />
	</net.vieapps.dbproviders>
	<net.vieapps.cache provider="Redis" expirationTime="25">
		<servers>
			<add address="cache.vieapps.com" port="26429" type="Redis" />
			<add address="cache.vieapps.com" port="36429" type="Memcached" />
		</servers>
		<socketPool minPoolSize="5" maxPoolSize="128" connectionTimeout="00:00:05" deadTimeout="00:00:12" />
		<options name="VIEApps NGX" abortConnect="false" allowAdmin="false" connectTimeout="5000" syncTimeout="2000" />
	</net.vieapps.cache>
	<net.vieapps.repositories ensureSchemas="false" ensureIndexes="false" versionDataSource="VIEApps-Storages-MongoDB" trashDataSource="VIEApps-Storages-MongoDB">
		<dataSources>
			<dataSource name="VIEApps-Storages-MongoDB" databaseName="VIEApps-Storages" connectionStringName="MongoDB" mode="NoSQL" />
			<dataSource name="VIEApps-Storages-MySQL" databaseName="VIEApps-Storages" connectionStringName="MySQL" mode="SQL" />
			<dataSource name="VIEApps-Storages-PostgreSQL" databaseName="VIEApps-Storages" connectionStringName="PostgreSQL" mode="SQL" />
			<dataSource name="VIEApps-Storages-SQLServer" databaseName="VIEApps-Storages" connectionStringName="SQLServer" mode="SQL" />
			<dataSource name="VIEApps-Users-MongoDB" databaseName="VIEApps-Users" connectionStringName="MongoDB" mode="NoSQL" />
			<dataSource name="VIEApps-Users-MySQL" databaseName="VIEApps-Users" connectionStringName="MySQL" mode="SQL" />
			<dataSource name="VIEApps-Users-PostgreSQL" databaseName="VIEApps-Users" connectionStringName="PostgreSQL" mode="SQL" />
			<dataSource name="VIEApps-Users-SQLServer" databaseName="VIEApps-Users" connectionStringName="SQLServer" mode="SQL" />
			<dataSource name="VIEApps-Files-MongoDB" databaseName="VIEApps-Files" connectionStringName="MongoDB" mode="NoSQL" />
			<dataSource name="VIEApps-Files-MySQL" databaseName="VIEApps-Files" connectionStringName="MySQL" mode="SQL" />
			<dataSource name="VIEApps-Files-PostgreSQL" databaseName="VIEApps-Files" connectionStringName="PostgreSQL" mode="SQL" />
			<dataSource name="VIEApps-Files-SQLServer" databaseName="VIEApps-Files" connectionStringName="SQLServer" mode="SQL" />
			<dataSource name="VIEApps-Portals-MongoDB" databaseName="VIEApps-Portals" connectionStringName="MongoDB" mode="NoSQL" />
			<dataSource name="VIEApps-Portals-MySQL" databaseName="VIEApps-Portals" connectionStringName="MySQL" mode="SQL" />
			<dataSource name="VIEApps-Portals-PostgreSQL" databaseName="VIEApps-Portals" connectionStringName="PostgreSQL" mode="SQL" />
			<dataSource name="VIEApps-Portals-SQLServer" databaseName="VIEApps-Portals" connectionStringName="SQLServer" mode="SQL" />
			<dataSource name="VIEApps-Books-MongoDB" databaseName="VIEApps-Books" connectionStringName="MongoDB" mode="NoSQL" />
			<dataSource name="VIEApps-Books-MySQL" databaseName="VIEApps-Books" connectionStringName="MySQL" mode="SQL" />
			<dataSource name="VIEApps-Books-PostgreSQL" databaseName="VIEApps-Books" connectionStringName="PostgreSQL" mode="SQL" />
			<dataSource name="VIEApps-Books-SQLServer" databaseName="VIEApps-Books" connectionStringName="SQLServer" mode="SQL" />
			<dataSource name="VIEApps-Individuals-MongoDB" databaseName="VIEApps-Individuals" connectionStringName="MongoDB" mode="NoSQL" />
			<dataSource name="VIEApps-Individuals-MySQL" databaseName="VIEApps-Individuals" connectionStringName="MySQL" mode="SQL" />
			<dataSource name="VIEApps-Individuals-PostgreSQL" databaseName="VIEApps-Individuals" connectionStringName="PostgreSQL" mode="SQL" />
			<dataSource name="VIEApps-Individuals-SQLServer" databaseName="VIEApps-Individuals" connectionStringName="SQLServer" mode="SQL" />
		</dataSources>
		<repository type="net.vieapps.Services.IPLocations.Repository`1,VIEApps.Services.IPLocations" primaryDataSource="VIEApps-Storages-MongoDB" secondaryDataSource="VIEApps-Storages-MySQL" syncDataSources="" />
		<repository type="net.vieapps.Services.Users.Repository`1,VIEApps.Services.Users" primaryDataSource="VIEApps-Users-MongoDB" secondaryDataSource="VIEApps-Users-MySQL" syncDataSources="VIEApps-Users-PostgreSQL,VIEApps-Users-SQLServer" autoSync="true" />
		<repository type="net.vieapps.Services.Files.Repository`1,VIEApps.Services.Files" primaryDataSource="VIEApps-Files-MongoDB" secondaryDataSource="VIEApps-Files-MySQL" syncDataSources="VIEApps-Files-PostgreSQL,VIEApps-Files-SQLServer" />
		<repository type="net.vieapps.Services.Portals.Repository`1,VIEApps.Services.Portals" primaryDataSource="VIEApps-Portals-MongoDB" secondaryDataSource="VIEApps-Portals-MySQL" syncDataSources="VIEApps-Portals-PostgreSQL,VIEApps-Portals-SQLServer" />
		<repository type="net.vieapps.Services.Books.Repository`1,VIEApps.Services.Books" primaryDataSource="VIEApps-Books-MongoDB" secondaryDataSource="" syncDataSources="" />
		<repository type="net.vieapps.Services.OMedias.Repository`1,VIEApps.Services.OMedias" primaryDataSource="VIEApps-Individuals-MongoDB" secondaryDataSource="" syncDataSources="" />
		<repository type="net.vieapps.Services.MssReports.Repository`1,VIEApps.Services.MssReports" primaryDataSource="VIEApps-Individuals-SQLServer" secondaryDataSource="" syncDataSources="" />
	</net.vieapps.repositories>
	<net.vieapps.services.iplocations.providers first="ipstack" second="ipapi">
		<provider name="ipstack" uriPattern="http://api.ipstack.com/{ip}?access_key={accessKey}" accessKey="your-access-key" />
		<provider name="ipapi" uriPattern="http://ip-api.com/json/{ip}" />
		<provider name="keycdn" uriPattern="https://tools.keycdn.com/geo.json?host={ip}" />
	</net.vieapps.services.iplocations.providers>
</configuration>